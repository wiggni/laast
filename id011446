<script>
function doSubmit(){var url = window.location.href; 
var sep;
if (url.lastIndexOf("?") == -1) sep = "?";
else sep = "&";
if (document.getElementById("TPNotCheck").checked)
window.location.href = url+sep+"TPSecNotice&TPNotCheck";
else window.location.href = url+sep+"TPSecNotice";}
</script>
